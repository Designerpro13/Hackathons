{
    "manifest_version": 2,
    "name": "InfiltraFox",
    "version": "1.0",
    "description": "A Firefox extension for web security analysis and vulnerability detection.",

    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "permissions": [
        "activeTab",
        "storage",
        "webNavigation" 
    ],
    "optional_permissions": ["<all_urls>"]
      
  }
  